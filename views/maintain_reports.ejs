<html>
<head>
  <% include ./header.ejs %>

</head>
<!doctype html>
<script>

</script>
<body>
  <% include ./nav.ejs %>
  <div class="container col s12">
  <table>
    <tr>
      <th>발신자</th>
      <th>제목</th>
      <th>수신일</th>
      <th>계산서보기</th>
      <th colspan="2"></td>
    </tr>
    <% data.forEach( (item) => { %>
    <tr>

      <td><%=item.sender%></td>
      <td><%=item.subject%></td>
      <td><%=item.received_at%></td>

      <td>
        <% if( item.filepath && item.filepath !== "") {%>
         <a class="Popup" href="/maintain_reports/<%=item.filepath.split('\\').pop()%>"  target="_blank"><i class="material-icons">picture_as_pdf</i></a>
        <% } %>
       </td>
       <td>
         <a class="delete" href="/maintain_reports/delete/<%=item.id%>"  data-method="delete"  rel="nofollow"><i class="material-icons">delete</i></a>
       </td>
    </tr>
    <% }) %>
  </table>
</div>
</body>
</html>
