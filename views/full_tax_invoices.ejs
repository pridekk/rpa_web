<html>
<head>
  <% include ./header.ejs %>

</head>
<!doctype html>
<script>

</script>
<body>
  <table>
    <tr>
      <th>수신회사</th>

      <th>수신품목</th>
      <th>관리번호</th>
      <th>월</th>
      <th>일</th>
      <th>금액</th>
      <th>세액</th>
      <th>총금액</th>
      <th>수신횟수</th>
      <th>계산서보기</th>
      <th></td>
    </tr>
    <% data.forEach( (item) => { %>
    <tr>

      <td><%=item.company%></td>

      <td><%=item.item_name%></td>
      <td><%=item.mgmt_id%></td>
      <td><%=item.total_price%></td>

      <td><%=item.month%></td>
      <td><%=item.day%></td>
      <td><%=item.price%></td>
      <td><%=item.tax%></td>
      <td><%=item.total_price%></td>
      <td><%=item.mail_count%></td>
      <td>
        <% if( item.filepath && item.filepath !== "") {%>
         <a class="Popup" href="/pdf/<%=item.filepath.split('\\').pop()%>"  target="_blank">계산서</a>
        <% } %>
       </td>
       <td>
         <a class="delete" href="/tax_invoices/delete/<%=item.invoice_id%>"  data-method="delete"  rel="nofollow">삭제</a>
       </td>
    </tr>
    <% }) %>
  </table>
</body>
</html>
